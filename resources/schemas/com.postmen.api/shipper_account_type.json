  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Shipper Account ID"
    },
    "address": {
      "$ref": "/shipper_account_address"
    },
    "slug": {
      "type": "string",
      "description": "The slug of the shipper_account"
    },
    "status": {
      "type": "string",
      "description": "The current status of the shipper account",
      "enum": [
        "enabled",
        "disabled",
        "deleted"
      ]
    },
    "description": {
      "type": "string",
      "description": "The description of the account",
      "minLength": 1,
      "maxLength": 35
    },
    "type": {
      "type": "string",
      "description": "The type of the shipper_account",
      "enum": [
        "user",
        "user_prepaid"
      ]
    },
    "timezone": {
      "type": "string",
      "description": "Use IANA â€” Time Zone Database"
    },
    "created_at": {
      "$ref": "/postmen_date"
    },
    "updated_at": {
      "$ref": "/postmen_date"
    }
  },
  "additionalProperties": false
}